<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/margenInt"
    tools:context="com.example.profesoresi.appalmacenamiento.ConsultaAEActivity">

    <TextView
        android:id="@+id/tvTituloExt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="Contenido del fichero "
        android:textSize="24sp" />

    <TextView
        android:id="@+id/tvContenidoExt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tvTituloExt"
        android:layout_marginTop="26dp"
        android:textSize="24sp" />
</RelativeLayout>
